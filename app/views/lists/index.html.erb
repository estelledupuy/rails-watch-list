<h1>Lists index</h1>
<% @lists.each do |list| %>
  <li><% if list.photo.attached? %>
    <%= cl_image_tag list.photo.key, height: 300, width: 400, crop: :fill %>
  <% else %>
   <img src="https://images.unsplash.com/photo-1571235908642-0459c9475ac3?ixlib=rb-4.0.3&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80">
  <% end %>
  <%= link_to list.name, list_path(list) %>
  </li>
<% end %>

<%= link_to "Create a new list", new_list_path %>
